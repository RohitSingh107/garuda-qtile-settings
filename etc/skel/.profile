[[ -f ~/.bashrc ]] && . ~/.bashrc
export BROWSER=firedragon
export TERM=alacritty
export MAIL=thunderbird
export QT_QPA_PLATFORMTHEME="qt5ct"
export GTK2_RC_FILES="$HOME/.gtkrc-2.0"
export XDG_CURRENT_DESKTOP=Unity

export ALTERNATE_EDITOR="micro"                      
export EDITOR="micro"              
export VISUAL="micro"           
